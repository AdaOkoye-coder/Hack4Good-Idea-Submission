  align-items: center;
  gap: 6px;
  padding: 6px 12px;
  border-radius: 8px;
  font-weight: 600;
  font-size: 14px;
  border: 1px solid #0a4661;
  background: #06354e;
  color: #e7ecf7;
  text-decoration: none;
  transition: background .15s ease, transform .1s ease;
}

.h4g-back .btn-back:hover {
  background: #0a4661;
  transform: translateX(-2px);
}

